{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/videoDetails", "dest": "/api/videoDetails.js" },
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/api/relatedVideos", "dest": "/api/relatedVideos.js" },
    { "src": "/api/channel", "dest": "/api/channel.js" },
    { "src": "/api/playlists", "dest": "/api/playlists.js" },
    { "src": "/api/comments", "dest": "/api/comments.js" },
    { "src": "/api/trending", "dest": "/api/trending.js" },
    { "src": "/api/ai", "dest": "/api/ai.js" }
  ]
}
